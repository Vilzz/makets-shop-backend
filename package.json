    "node-api",
    "space-ships",
    "models"
  ],
  "author": "Baklanov Dmitry",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.1.6",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "mongoose-sequence": "^5.2.2",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.1",
    "nodemailer": "^6.4.6",
    "rotating-file-stream": "^2.0.2",
    "slugify": "^1.4.0",
    "xss-clean": "^0.1.1"
